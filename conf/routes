# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# DO NOT CHANGE
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

# DO NOT CHANGE

# Risk information

GET     /                           controllers.GeneralController.index

GET     /risk/game                  controllers.GeneralController.game

# Game specific
GET     /risk/game/description      controllers.GeneralController.description

GET     /risk/game/rules            controllers.GeneralController.rules